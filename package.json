{
  "name": "storkio-react",
  "version": "1.0.0",
  "description": "React generator",
  "main": "src/index.js",
  "scripts": {
    "test": "yarn lint && yarn jest",
    "jest": "jest --testMatch **/__tests__/**/*.spec.js",
    "build": "rimraf dist && yarn webpack",
    "precommit": "lint-staged",
    "commitmsg": "commitlint -e",
    "lint": "eslint src --fix",
    "dev": "webpack-dev-server --open",
    "prod": "yarn build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/storkio/storkio-react.git"
  },
  "keywords": [
    "react",
    "starter",
    "generator"
  ],
  "author": "storkio",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/storkio/storkio-react/issues"
  },
  "homepage": "https://github.com/storkio/storkio-react#readme",
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@commitlint/cli": "^7.0.0",
    "@commitlint/config-angular": "^7.0.1",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^8.0.4",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "css-loader": "^1.0.0",
    "dotenv-extended": "^2.0.2",
    "enzyme": "^3.3.0",
    "eslint": "^5.3.0",
    "eslint-config-noamokman": "^8.0.1",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jest": "^21.18.0",
    "eslint-plugin-lodash": "^2.7.0",
    "eslint-plugin-react": "^7.10.0",
    "eslint-plugin-unicorn": "^5.0.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "jest": "^23.4.2",
    "jest-enzyme": "^6.0.2",
    "lint-staged": "^7.2.0",
    "style-loader": "^0.21.0",
    "supertest": "^3.1.0",
    "webpack": "^4.16.4",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.5"
  },
  "dependencies": {
    "@material-ui/core": "^1.4.3",
    "axios": "^0.18.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-hot-loader": "^4.3.4",
    "react-router": "^4.3.1",
    "reflexbox": "^3.0.1",
    "styled-components": "^3.4.1"
  }
}
